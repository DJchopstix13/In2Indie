html, body
    font: 16px Helvetica, Arial, sans-serif  
    height: 100%
    width: 100%
    overflow: hidden

.navbar-default
    background-color: rgb(255, 165, 0) !important
    margin-bottom: 0 !important


.table td
    border-top: none 

#wrap
    height: 100%
    width: 100%

a:hover
    opacity: 0.5

.feature-div
    padding: 6px
    background-color: rgba(136, 159, 151, .1)
    border-radius: 10px

// Content and main-content styles

#content
    height:100%
    width:100%
    background-image:url('/images/texture3.png')

#main-content
    margin-left: 10%
    background-color: rgb(255, 153, 51)
    position: fixed
    width:80%
    height: 90%
    overflow-y: auto !important

#page-content
    margin-top: 10px
    margin-left: 30px
    padding-top: 3px


#contentBox
    height: 80%
    width: 90%
    margin: 0 auto


.underline
    border-bottom: 5px solid rgba(0, 0, 0, .80)
    margin: 5%
  
#user-profile
    padding: 10px
    background-color: rgba(19,164,157, .12)

.right-label
    text-align: right !important

.sub-nav
    padding-left: 5px
    background-color:rgba(19,164,157, .12)

.page-title
    text-align: center
    padding-top: 10px
    padding-bottom: 10px
    background-color: rgba(91, 192, 222, 0.2)
